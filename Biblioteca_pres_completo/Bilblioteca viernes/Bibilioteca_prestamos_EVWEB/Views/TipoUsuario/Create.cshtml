
@{
    ViewBag.Title = "Create";
}

<h1 class="text-center">Crear Nuevo Tipo Usuario</h1>
<form method="post" class="needs-validation" style="width:60%"
    :" novalidate>
    <div class="mb-3">
        <label class="form-label">Tipo Usuario</label>
        <input type="text" id="nombre" name="nombre" class="form-control" required placeholder="Ingrese Usuario" />
        <div class="text-danger">@ViewBag.Error</div>
    </div>
    <div>
        <a href="/TipoUsuario/Index" class="btn btn-primary">VOLVER</a>
        <button type="submit" class="btn btn-success">GUARDAR</button>
    </div>
</form>
<script src="~/Scripts/validacion.js"></script>

