@model Bibilioteca_prestamos_EVWEB.Models.TipoUsuario
@{
    ViewBag.Title = "Delete";
}

<h2 class="text-center">ELIMINAR @Model.Nombre</h2>
<div class="text-center">
    <h2>¿Está seguro que desea eliminar el tipo de usuario <b>@Model.Nombre</b>?</h2>
    <form method="post">
        <a href="/TipoUsuario/Index" class="btn btn-primary">VOLVER</a>
        <button class="btn btn-danger">ELIMINAR</button>
    </form>
</div>
